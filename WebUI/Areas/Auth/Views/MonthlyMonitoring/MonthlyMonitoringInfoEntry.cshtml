<link href="~/Content/bootstrap-datetimepicker.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/jquery-ui-1.11.4.min.js"></script>
<script src="~/Scripts/moment.js"></script>
<script src="~/Scripts/knockout-date-bindings.js"></script>
<link href="~/Content/layout-modifications.css" rel="stylesheet" />
<script src="~/Areas/Auth/Scripts/MonthlyMonitoring/MonthlyMonitoringEntry.js"></script>

@{
    ViewBag.Title = "WorkrecordEntry";
    Layout = "~/Views/Shared/_LayoutContentPage.cshtml";
}

<div id="mmInfoEntry">
    <section class="rm">
        <div class="container-fluid ">
            <fieldset>
                <div class="row">
                    <div class="col-xs-12">
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="table-header">
                                    Dengue Protection Monitoring System
                                </div>
                                <div id="dynamic-table_wrapper" class="dataTables_wrapper form-inline no-footer">


                                    <table class="table table-striped table-bordered table-hover dataTable no-footer" frame="box">
                                        <thead>
                                            <tr>
                                                <th style="font-weight:bolder">Name of Structure</th>
                                                <th>Pond</th>
                                                <th>Wastage</th>
                                                <th>Medical</th>
                                                <th>Office</th>
                                                <th>Strill</th>
                                                <th>Curing</th>
                                                <th>Construction</th>
                                            </tr>
                                        </thead>
                                        <tbody data-bind="foreach:AssetList">
                                            <tr>
                                                <td style="display:none"><input type="hidden" data-bind="value:Id" /></td>
                                                <td style="text-align:left;" data-bind="text: OfficeAssetName"></td>
                                                <td style="text-align:left;" data-bind="text: IsPondCleanUpName"></td>
                                                <td style="text-align:left;" data-bind="text: IsWastageCleanUpName"></td>
                                                <td style="text-align:left;" data-bind="text: IsMedicalCollegeCleanUpName"></td>
                                                <td style="text-align:left;" data-bind="text: IsOfficeAndHouseholdCleanUpName"></td>
                                                <td style="text-align:left;" data-bind="text: IsStillWaterCleanUpName"></td>
                                                <td style="text-align:left;" data-bind="text: IsCuringWaterCleanUpName"></td>
                                                <td style="text-align:left;" data-bind="text: IsUnderConstructionBuildingCleanUpName"></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </section>

</div>


