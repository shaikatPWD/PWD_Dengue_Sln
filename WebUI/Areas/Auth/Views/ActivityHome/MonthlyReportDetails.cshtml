<link href="~/Content/bootstrap-datetimepicker.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/jquery-ui-1.11.4.min.js"></script>
<script src="~/Scripts/moment.js"></script>
<script src="~/Scripts/knockout-date-bindings.js"></script>
<link href="~/Content/layout-modifications.css" rel="stylesheet" />
<script src="~/Areas/Auth/Scripts/Activity/MonthlyReport.js"></script>
<style type="text/css">
    .tdStyle {
        font-size: 14px !important;
        color: green !important;
        text-align: left;
    }

    .thStyle {
        font-size: 14px !important;
        color: #1D5987 !important;
    }
</style>
@{
    ViewBag.Title = "MonthlyReportDetails";
    Layout = "~/Views/Shared/_LayoutContentPage.cshtml";
}

<div id="mmInfoEntry">
    <section class="rm">
        <div class="container-fluid ">
            <fieldset>
                <div class="row">
                    <div class="col-xs-12">
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="table-header">
                                    Dengue Protection Monitoring System
                                </div>
                                <div id="dynamic-table_wrapper" class="dataTables_wrapper form-inline no-footer">


                                    <table class="table table-striped table-bordered table-hover dataTable no-footer" frame="box">
                                        <thead>
                                            <tr>
                                                <th class="thStyle">Building Name</th>
                                                <th class="thStyle">Date</th>
                                                <th class="thStyle">পুকুর/জলাশয়/লেক/খাল কচুরীপানা মুক্ত ও পরিষ্কার করা</th>
                                                <th class="thStyle">খোলা যায়গার নর্দমা, ডাস্টবিনের বর্জ্য অপসারণ</th>
                                                <th class="thStyle">খোলা ড্রেইন পরিষ্কার করণ</th>
                                                <th class="thStyle">ঝোপ-ঝাড় ও আগাছা পরিষ্কারকরণ</th>
                                                <th class="thStyle">গাছপালা ঝোপঝাড়ে fogging করা</th>
                                                <th class="thStyle">খোলা ড্রেইনে লার্ভা ধ্বংসকারী ওষধ নিয়মিতভাবে ছিটানো</th>
                                                <th class="thStyle">আবদ্ধ পানি অপসারণ</th>
                                            </tr>
                                        </thead>
                                        <tbody data-bind="foreach:PeriodList">
                                            <tr>
                                                <td class="tdStyle" hidden><input type="hidden" data-bind="value:Id" /></td>
                                                <td class="tdStyle" data-bind=" text:OfficeAssetName"></td>
                                                <td class="tdStyle" data-bind=" text:moment(Date).format('DD/MM/YYYY')"></td>
                                                <td class="tdStyle" data-bind=" text:IsPondsCleanUpName"></td>
                                                <td class="tdStyle" data-bind=" text:IsWastageCleanUpName"></td>
                                                <td class="tdStyle" data-bind=" text:IsMedicalCollegeCleanUpName"></td>
                                                <td class="tdStyle" data-bind=" text:IsOfficeAndHouseholdCleanUpName"></td>
                                                <td class="tdStyle" data-bind=" text:IsStillWaterCleanUpName"></td>
                                                <td class="tdStyle" data-bind=" text:IsCuringWaterCleanUpName"></td>
                                                <td class="tdStyle" data-bind=" text:IsUnderConstructionBuildingCleanUpName"></td>

                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </section>

</div>

